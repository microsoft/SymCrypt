{
    "terminal.integrated.defaultProfile.windows": "SymCrypt Profile",
    "terminal.integrated.profiles.windows": {
        "SymCrypt Profile": {
            "args": [
                "-noe",
                "-c",
                "&{ if (test-path ${workspaceFolder}\\.venv\\profile.ps1) { . ${workspaceFolder}\\.venv\\profile.ps1 } }"
            ],
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "overrideName": true,
        }
    },
    "terminal.integrated.defaultProfile.linux": "SymCrypt Profile",
    "terminal.integrated.profiles.linux": {
        "SymCrypt Profile": {
            "args": [
                "-c",
                "[ -f ./.venv/bin/activate ] && . ./.venv/bin/activate; exec bash"
            ],
            "path": "bash",
            "source": "bash",
            "icon": "terminal-bash",
            "overrideName": true,
        }
    },
}