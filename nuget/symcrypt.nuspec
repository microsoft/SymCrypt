  <files>
    <!-- Documentation -->
    <file src="..\README.md" target="" />
    <file src="..\CHANGELOG.md" target="" />
    <file src="..\NOTICE.txt" target="" />
    
    <!-- Header files (taken from Windows AMD64, they're the same regardless of arch) -->
    <file src="..\pkg\win-x64\inc\symcrypt.h" target="lib\native\include\" />
    <file src="..\pkg\win-x64\inc\symcrypt_low_level.h" target="lib\native\include\" />
    <file src="..\pkg\win-x64\inc\symcrypt_internal.h" target="lib\native\include\" />
    <file src="..\pkg\win-x64\inc\symcrypt_internal_shared.inc" target="lib\native\include\" />
    
    <!-- Build targets -->
    <file src="build\native\Microsoft.SymCrypt.targets" target="build\native\" />
    
    <!-- Windows AMD64 (x86-64) runtime -->
    <file src="..\pkg\win-x64\dll\symcrypt.dll" target="runtimes\win-x64\native\" />
    <file src="..\pkg\win-x64\dll\symcrypt.lib" target="runtimes\win-x64\native\" />
    <file src="..\pkg\win-x64\dll\symcrypt.pdb" target="runtimes\win-x64\native\" />

