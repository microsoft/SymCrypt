  <metadata>
    <id>Microsoft.SymCrypt</id>
    <version>0.0.0</version> <!-- Should be overridden during package creation with version flag -->
    <title>SymCrypt</title>
    <authors>Microsoft Corporation</authors>
    <owners>Microsoft Corporation</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/microsoft/SymCrypt</projectUrl>
    <repository type="git" url="https://github.com/microsoft/SymCrypt" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <readme>README.md</readme>
    <description>SymCrypt cryptographic library</description>
    <summary>SymCrypt cryptographic library</summary>
    <copyright>Copyright (c) Microsoft Corporation. All rights reserved.</copyright>
    <tags>symcrypt cryptography crypto native windows linux microsoft security</tags>
    <dependencies>
      <group targetFramework="native0.0" />
