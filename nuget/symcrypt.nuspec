    <file src="..\pkg\win-arm64\dll\symcrypt.pdb" target="runtimes\win-arm64\native\" />
    
    <!-- Headers for non-MSVC compilers -->
    <!-- <file src="..\pkg\linux-x64\inc\symcrypt_no_sal.h" target="lib\native\include\" /> -->

    <!-- Linux AMD64 (x86-64) runtime -->
